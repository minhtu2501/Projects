<div class="container">
	<div class="jumbotron">
    <% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
    <% end %>
		<div class="col-md-3"></div>
		<div class="col-md-6">
			<h1 id="welcome"><strong>WELCOME!</strong></h1>
			<%= form_for @member  do |f| %>
   	 		    <%= f.label :Name %>
     	   	  <%= f.text_field :name, class: "form-control"%>

     		    <%= f.label :Age %>
      		  <%= f.text_field :age, class: "form-control"%>

            <%= f.label :Adress %>
            <%= f.text_field :address, class: "form-control"%>

      		  <%= f.label :Email %>
      		  <%= f.email_field :email, class: "form-control" %>

      		  <%= f.label :Tel%>
      		  <%= f.text_field :tel, class: "form-control"%>

      		  <%= f.submit "Create my account", class: "btn btn-primary form-control" %>
   		 	  <% end %>
		</div>
		<div class="col-md-3"></div>
	</div>	
</div>